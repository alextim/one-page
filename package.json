{
  "name": "one-page",
  "version": "0.1.0",
  "description": "One page React app with fixed header and scroll navigation",
  "keywords": [
    "react",
    "fixed header",
    "one page",
    "scroll navigation"
  ],
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "disable-scroll": "^0.5.1",
    "email-validator": "^2.0.4",
    "emotion-theming": "^10.0.27",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^4.3.0",
    "i18next-http-backend": "^1.0.15",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.6.0",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:stats": "webpack --env production --json > stats.json",
    "post-build": "react-snap",
    "deploy": "firebase deploy --only hosting",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "stylelint": "stylelint --syntax css-in-js src/**/*.{js,jsx,ts,tsx}",
    "analyze": "node ./scripts/analyze.js",
    "stats": "node ./scripts/stats.js",
    "dup": "node ./scripts/dup.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "reactSnap": {
    "inlineCss": true
  },
  "browserslist": {
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 edge version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alextim/one-page.git"
  },
  "author": "alex tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alextim/one-page/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "audit-ci": "^3.0.1",
    "chalk": "^4.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-snap": "^1.23.0",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  }
}
